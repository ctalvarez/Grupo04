<p id="notice"><%= notice %></p>

<h1>Todas nuestras series :D</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @series.each do |series| %>
      <tr>
        <td><%= series.name %></td>
        <td><%= series.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
